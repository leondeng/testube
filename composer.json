{
    "name": "leondeng/testube",
    "version": "1.0.0",
    "description": "A Test Driven Developing Tool",
    "keywords":
    [
        "TDD",
        "testing"
    ],
    "require": {
        "symfony/symfony": "2.7.*",
        "doctrine/inflector": "^1.0",
        "phpunit/phpunit": "^4.7"
    },
    "require-dev": {
        "leondeng/version_bump_test": "^0.1"
    },
    "repositories": [
        {
            "url": "git@github.com:leondeng/ver_bump_test.git",
            "type": "git"
        }
    ],
    "autoload":
    {
        "psr-0":
        {
            "LeonDeng\\TesTube": "lib/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    }
}
